#include <stdio.h>

int main()
{
    int n;
    int count=0;
    int sum=0;
    double avg;
    
    while(1){
        scanf("%d", &n);
        sum = sum + n;
        count++;
        if (count==4) break;
    }
    avg = sum / (double)count;
    printf("총합 : %d\n", sum);
    printf("평균 : %.1f", avg);
    return 0;
}
